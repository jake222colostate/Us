# When true, demo data will be used if Supabase credentials are missing or
# the backend is unreachable.
VITE_ENABLE_DEMO=true

# Supabase project URL and anon key used for authentication and database
# access. Obtain these from the Supabase dashboard. Both values are required
# for the live backend to function.
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=
