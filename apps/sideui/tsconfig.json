{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/ui/*": [
        "../../packages/ui/src/*"
      ],
      "@/components/*": [
        "../../packages/ui/src/components/*"
      ],
      "@/hooks/*": [
        "../../packages/ui/src/*"
      ]
    },
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false,
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "react-jsx"
  }
}