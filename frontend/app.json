{
  "expo": {
    "name": "US Frontend",
    "slug": "us-frontend",
    "scheme": "usapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "platforms": ["ios", "android", "web"],
    "experiments": {
      "typedRoutes": true
    },
    "plugins": ["expo-router"],
    "web": {
      "bundler": "metro"
    }
  }
}
